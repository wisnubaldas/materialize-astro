{
  "extends": "astro/tsconfigs/strict",
   "compilerOptions": {
    "baseUrl": ".",
    "paths": {
        "~/*": ["src/*"],
        "@css/*": ["src/assets/css/*"],
        "@js/*": ["src/assets/js/*"],
        "@json/*": ["src/assets/json/*"],
        "@img/*": ["src/assets/img/*"],
        "@svg/*": ["src/assets/svg/*"],
        "@vendor/*": ["src/assets/vendor/*"],
        "@components/*": ["src/components/*"],
        "@layouts/*": ["src/layouts/*"],
        "@pages/*": ["src/pages/*"],
      },
    },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}
