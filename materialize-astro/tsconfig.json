{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@modules/*": ["node_modules/*"],
      "@css/*": ["src/assets/css/*"],
      "@js/*": ["src/assets/js/*"],
      "@json/*": ["src/assets/json/*"],
      "@img/*": ["src/assets/img/*"],
      "@svg/*": ["src/assets/svg/*"],
      "@vendor/*": ["src/assets/vendor/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"]
    }
  },
  "moduleResolution": "node",
  "module": "ESNext",
  "target": "ESNext",
  "jsx": "preserve",
  "strict": true,
  "allowJs": true,
  "esModuleInterop": true,
  "resolveJsonModule": true,
  "isolatedModules": true,
  "skipLibCheck": true,
  "include": ["src", "astro.config.*", "env.d.ts", ".astro/types.d.ts", "**/*", "package.json"],
  "exclude": ["dist"]
}
